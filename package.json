{
  "name": "extract-git-treeish",
  "description": "extract git tree-ish (commit, branch, or tag) into dir",
  "version": "0.0.0",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "dependencies": {
    "tar": "^6.0.0"
  },
  "devDependencies": {
    "@twada/mocha-spec-tree-reporter": "^0.1.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^7.0.0",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0",
    "semistandard": "^14.0.0",
    "snazzy": "^8.0.0"
  },
  "keywords": [
    "git"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "fmt": "semistandard --fix",
    "lint": "semistandard --verbose | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:doc": "mocha test --reporter @twada/mocha-spec-tree-reporter",
    "test:unit": "mocha test --require intelli-espower-loader"
  },
  "semistandard": {
    "globals": [
      "describe",
      "beforeEach",
      "afterEach",
      "it"
    ]
  }
}
