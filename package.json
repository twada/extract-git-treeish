{
  "name": "extract-git-treeish",
  "description": "extract git tree-ish (commits, branches, or tags) into target directory",
  "version": "2.0.0",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "bugs": "https://github.com/twada/extract-git-treeish/issues",
  "dependencies": {
    "tar": "^6.1.9"
  },
  "devDependencies": {
    "@twada/mocha-spec-tree-reporter": "^0.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/tar": "^6.1.4",
    "mocha": "^10.0.0",
    "rimraf": "^4.0.0",
    "rollup": "^3.0.0",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "src",
    "dist",
    "package.json"
  ],
  "homepage": "http://github.com/twada/extract-git-treeish",
  "keywords": [
    "git"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./src/index.mjs",
    "require": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/twada/extract-git-treeish.git"
  },
  "scripts": {
    "preversion": "npm run build && npm test",
    "build": "rimraf dist && rollup -c",
    "fmt": "semistandard --fix src test",
    "lint": "semistandard --verbose src test | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:doc": "mocha test --reporter @twada/mocha-spec-tree-reporter",
    "test:unit": "mocha test"
  },
  "semistandard": {
    "globals": [
      "describe",
      "context",
      "beforeEach",
      "afterEach",
      "it"
    ]
  }
}
